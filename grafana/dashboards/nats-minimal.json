{
  "uid": "nats-minimal",
  "title": "NATS - Minimal",
  "tags": ["nats", "poc"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Connessioni attive",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum({job=\"nats\",__name__=~\"(gnatsd|nats)_varz_connections\"})",
          "legendFormat": "connections"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Messaggi / s",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 10, "w": 16, "x": 8, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({job=\"nats\",__name__=~\"(gnatsd|nats)_varz_in_msgs\"}[1m]))",
          "legendFormat": "in_msgs/s"
        },
        {
          "refId": "B",
          "expr": "sum(rate({job=\"nats\",__name__=~\"(gnatsd|nats)_varz_out_msgs\"}[1m]))",
          "legendFormat": "out_msgs/s"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops" },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Bytes / s",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 10 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({job=\"nats\",__name__=~\"(gnatsd|nats)_varz_in_bytes\"}[1m]))",
          "legendFormat": "in_bytes/s"
        },
        {
          "refId": "B",
          "expr": "sum(rate({job=\"nats\",__name__=~\"(gnatsd|nats)_varz_out_bytes\"}[1m]))",
          "legendFormat": "out_bytes/s"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "Bps" },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    }
  ]
}
